         scale: 1;
         description
         {
            state: "default" 0.0;
            min: POPUP_CONTENT_DEFAULT_PADDING_LEFT_MIN_INC 0;
            fixed: 1 0;
            rel1.relative: 0.0 0.0;
            rel2.relative: 0.0 1.0;
            align: 0.0 0.0;
         }
      }
      part
      {
         name: "pad_r";
         type: SPACER;
         scale: 1;
         description
         {
            state: "default" 0.0;
