#define POPUP_CONTENT_DEFAULT_PADDING_LEFT_MIN_INC 15
#define POPUP_EDITFIELD_LAYOUT_MINMAX_HEIGHT_INC 96
group
{
   name: "server_layout";
   parts
   {
      part
      {
         name: "button1";
         type: SWALLOW;
         scale: 1;
         description
         {
            rel1.relative: 0.01 0.01;
            rel2.relative: 0.5 0.09;
         }
      }
      part
      {
         name: "button2";
         type: SWALLOW;
         scale: 1;
         description
         {
            rel1.relative: 0.51 0.01;
            rel2.relative: 1.0 0.09;
         }
      }
      part
      {
         name: "text1";
         type: TEXT;
         description
         {
            rel1.relative: 0.01 0.1;
            rel2.relative: 0.3 0.2;
            state: "default" 0.0;
            text
            {
               text: "Acceptor";
               font: "Sans";
               size: 35;
               align: 0.0 0.0;
            }
         }
      }
      part
      {
         name: "radio1";
         type: SWALLOW;
         scale: 1;
         description
         {
            rel1.relative: 0.22 0.1;
            rel2.relative: 0.6 0.15;
         }
      }
      part
      {
         name: "radio2";
         type: SWALLOW;
         scale: 1;
         description
         {
            rel1.relative: 0.61 0.1;
            rel2.relative: 0.99 0.15;
         }
      }
      part
      {
         name: "list";
         type: SWALLOW;
         scale: 1;
         description
         {
            rel1.relative: 0.01 0.16;
            rel2.relative: 0.99 0.7;
         }
      }
      part
      {
         name: "listnew";
         type: SWALLOW;
         scale: 1;
         description
         {
            rel1.relative: 0.01 0.16;
            rel2.relative: 0.99 0.7;
         }
      }
      part
      {
         name: "log_bg";
         type: RECT;
         scale: 1;
         description
         {
            state: "default" 0.0;
            rel1.relative: 0.01 0.71;
            rel2.relative: 0.98 1.0;
            color: 220 220 220 255;
         }
      }
      part
      {
         name: "log";
         type: SWALLOW;
         scale: 1;
         description
         {
            rel1.to: "log_bg";
            rel2.to: "log_bg";
         }
      }
   }
}
group
{
   name: "popup_datetime_text";
   parts
   {
      part
      {
         name: "pad_l";
